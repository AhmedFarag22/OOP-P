#include<iostream>
using namespace std;

class rectangle {

private:

	int h, w;

public:

	rectangle() :w(0), h(0) {
		cout << "w  = " << w << endl << "h =  " << h << endl;
	}

	rectangle(int a, int b) :w(0), h(0) {

		w = a;
		h = b;
	}

	~rectangle() {

		cout << w << "  " << h << "a r h b d ";

	}


};

int main() {

	rectangle r1(3, 4), r2(5, 6);
	//rectangle r3();

}
