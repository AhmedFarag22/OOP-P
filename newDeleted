#include<iostream>
using namespace std;



int main() {


	int* p1, * p2;
	p1 = new int;

	*p1 = 10;

	cout << &p1 << endl;
	cout << "memory location  " << p1 << endl;
	cout << "contains  " << *p1 << endl;
	p2 = new int;
	*p2 = 11;
	cout << &p2 << endl;
	cout << "memory location  " << p2 << endl << '\n' << "contains" << p2 << endl;
	delete p1;
	delete p2;


}
